{
  "name": "review-approval",
  "version": "1.0.0",
  "description": "",
  "license": "MIT",
  "scripts": {
    "test": "jest --verbose --coverage",
    "testBackend": "cd __tests__ && node endPointTest.js",
    "lint": "tsc && eslint \"src/**/*.{ts,tsx}\" --fix"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier-eslint --write",
      "git add"
    ]
  },
  "resolutions": {
    "@types/react": "16.9.11",
    "@types/react-native": "0.60.22"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.19.0",
    "@react-native-community/eslint-config": "^3.0.1",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/jest-native": "^4.0.12",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-native": "^11.5.0",
    "@types/cleave.js": "^1.4.7",
    "@types/jest": "^29.2.4",
    "@typescript-eslint/eslint-plugin": "4.0.0",
    "babel-jest": "25.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.10.0",
    "eslint-config-react-app": "5.2.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-eslint-comments": "3.1.2",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jest": "23.7.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-react-native": "3.8.1",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "26.6.3",
    "jest-sonar-reporter": "^2.0.0",
    "jsdom": "11.12.0",
    "jsdom-global": "3.0.2",
    "lint-staged": "10.0.7",
    "prettier": "^2.5.0",
    "prettier-eslint-cli": "5.0.0",
    "raf": "^3.4.1",
    "ts-jest": "^25.2.0",
    "typescript": "4.1.3"
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "test-reporter.xml",
    "indent": 4
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "__tests__/tsconfig.base.json"
      }
    },
    "testResultsProcessor": "jest-sonar-reporter",
    "setupFilesAfterEnv": [
      "<rootDir>/test-setup.js"
    ],
    "testEnvironment": "jsdom",
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*steps*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/coverage/",
      "endPointTest.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native-community|@react-native-picker)"
    ],
    "cacheDirectory": ".jest/cache",
    "collectCoverageFrom": [
      "!babel.config.js",
      "!node_modules/**/*",
      "!build/**",
      "!node_modules/**/**/*",
      "!coverage/**/*",
      "!__tests__/**/*",
      "!**/*Controller*"
    ],
    "moduleNameMapper": {}
  }
}
