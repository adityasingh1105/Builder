{
  "name": "order-management",
  "version": "1.0.1",
  "repository": "https://gitlab.builder.ai/builder/builder-bx/Bx/blocks/react-native/development/react-native-master-app/packages/blocks/ordermanagement.git",
  "license": "MIT",
  "scripts": {
    "test": "jest --verbose --coverage",
    "testBackend": "cd __tests__ && node endPointTest.js",
    "lint": "tsc && eslint 'src/**/*.{ts,tsx}' --fix"
  },
  "resolutions": {
    "@types/react": "file:../core/node_modules/@types/react",
    "@types/react-native": "file:../core/node_modules/@types/react-native"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/jest-native": "^4.0.12",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-native": "^9.1.0",
    "@types/jest": "^25.1.3",
    "babel-jest": "25.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-sonar-reporter": "^2.0.0",
    "raf": "^3.4.1",
    "ts-jest": "^25.2.0",
    "typescript": "4.1.3"
  },
  "dependencies": {},
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "test-reporter.xml",
    "indent": 4
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "__tests__/tsconfig.base.json"
      }
    },
    "testResultsProcessor": "jest-sonar-reporter",
    "setupFilesAfterEnv": [
      "<rootDir>/test-setup.js"
    ],
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testEnvironment": "jsdom",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/coverage/",
      "endPointTest.js"
    ],
    "cacheDirectory": ".jest/cache",
    "collectCoverageFrom": [
      "!babel.config.js",
      "!node_modules/**/*",
      "!build/**",
      "!node_modules/**/**/*",
      "!coverage/**/*",
      "!__tests__/**/*"
    ]
  }
}
